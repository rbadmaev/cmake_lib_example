set(PACKAGE_NAME "MyLib")
set(PACKAGE_VERSION "1.0")

if (${PACKAGE_NAME} STREQUAL ${PACKAGE_FIND_NAME})
    if (${PACKAGE_FIND_VERSION} STREQUAL ${PACKAGE_VERSION})
        message("MATCH VERSION EXACT")
        set(PACKAGE_VERSION_EXACT TRUE)
    else()
        message("EXACT VERSION DOES NOT MATCH")
        set(PACKAGE_VERSION_EXACT FALSE)
    endif()

    list(APPEND COMPATIBLE_VERSIONS ${PACKAGE_VERSION})
    list(APPEND COMPATIBLE_VERSIONS "0.9.0")
    list(APPEND COMPATIBLE_VERSIONS "0.9.1")
    list(APPEND COMPATIBLE_VERSIONS "0.9.2")
    if (${PACKAGE_FIND_VERSION} IN_LIST COMPATIBLE_VERSIONS)
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
    else()
        set(PACKAGE_VERSION_COMPATIBLE FALSE)
    endif()

    if (PACKAGE_VERSION_COMPATIBLE)
        set(PACKAGE_VERSION_UNSUITABLE, FALSE)
    else()
        set(PACKAGE_VERSION_UNSUITABLE, TRUE)
    endif()
endif()

